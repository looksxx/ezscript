"Simple Window": {
  "prefix": "simpleWindow",
  "body": [
    "createWindow(${1:800}, ${2:600}, \"${3:My Window}\")",
    "setBackground(\"${4:white}\")",
    "",
    "${5:# Draw your graphics here}",
    "",
    "showWindow()"
  ],
  "description": "Simple window template"
},

"Animated Window": {
  "prefix": "animatedWindow",
  "body": [
    "createWindow(${1:800}, ${2:600}, \"${3:Animation}\")",
    "setBackground(\"white\")",
    "",
    "let x be 0",
    "",
    "loop ${4:100} times:",
    "    clearWindow()",
    "    drawCircle(x, ${5:300}, ${6:20}, \"black\", \"red\")",
    "    x += ${7:5}",
    "    updateWindow()",
    "    wait 0.05 seconds",
    "",
    "showWindow()"
  ],
  "description": "Animated window template"
},

"Keyboard Controlled": {
  "prefix": "keyboardControl",
  "body": [
    "let x be 400",
    "let y be 300",
    "",
    "function moveLeft():",
    "    x -= 10",
    "",
    "function moveRight():",
    "    x += 10",
    "",
    "function moveUp():",
    "    y -= 10",
    "",
    "function moveDown():",
    "    y += 10",
    "",
    "createWindow(800, 600, \"Keyboard Control\")",
    "setBackground(\"white\")",
    "",
    "onKeyDown(\"Left\", moveLeft)",
    "onKeyDown(\"Right\", moveRight)",
    "onKeyDown(\"Up\", moveUp)",
    "onKeyDown(\"Down\", moveDown)",
    "",
    "showWindow()"
  ],
  "description": "Keyboard controlled object template"
},
