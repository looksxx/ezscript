  "Number Guessing Game": {
    "prefix": "game",
    "body": [
      "# Number Guessing Game",
      "let secret be random(1, 100)",
      "let attempts be 0",
      "",
      "print \"Guess the number between 1 and 100!\"",
      "",
      "while attempts < 10 then",
      "    let guess_str be input(\"Your guess: \")",
      "    let guess be int(guess_str)",
      "    attempts += 1",
      "    ",
      "    if guess is equal to secret then",
      "        print \"Correct! You won in {attempts} attempts!\"",
      "        break",
      "    elif guess greater than secret then",
      "        print \"Too high!\"",
      "    else",
      "        print \"Too low!\""
    ],
    "description": "Number guessing game template"
  }
